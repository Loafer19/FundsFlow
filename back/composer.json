{
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "laravel/framework": "^12.3",
        "php": "^8.2"
    },
    "require-dev": {
        "laravel/pint": "^1.21"
    },
    "scripts": {
        "pint": "./vendor/bin/pint -v"
    },
    "type": "project"
}
