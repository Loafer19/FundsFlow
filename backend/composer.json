{
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^8.2",
        "laravel/framework": "12.4.0",
        "laravel/sanctum": "4.0.8"
    },
    "require-dev": {
        "laravel/pint": "1.21.2"
    },
    "scripts": {
        "pint": "./vendor/bin/pint -v"
    },
    "type": "project"
}
